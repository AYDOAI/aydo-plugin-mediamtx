{
  "name": "MediaMTX",
  "type": 5,
  "icon": "hub",
  "class_name": "mediamtx",
  "module": "mediamtx",
  "cloud": true,
  "support_autoupdate": true,
  "autoupdate_interval": 15000,
  "mac_address": true,
  "capabilities": [
    {
      "ident": "motion"
    }
  ],
  "sort_index": 5,
  "preview": true,
  "power_switch": true,
  "commands": [
    {
      "command": "preview"
    },
    {
      "command": "power"
    },
    {
      "command": "channel"
    },
    {
      "command": "format"
    },
    {
      "command": "up"
    },
    {
      "command": "down"
    },
    {
      "command": "left"
    },
    {
      "command": "right"
    },
    {
      "command": "push_button_1"
    },
    {
      "command": "mode_1"
    },
    {
      "command": "snapshot"
    }
  ],
  "display": [
    {
      "functions": [
        {
          "view": "camera",
          "type": "camera"
        }
      ]
    }
  ],
  "settings": [
    {
      "key": "address",
      "name": "RTSP Address",
      "type": "text",
      "required": true
    },
    {
      "key": "username",
      "name": "User Name",
      "type": "text",
      "autocomplete": "new-password"
    },
    {
      "key": "password",
      "name": "Password",
      "type": "password",
      "autocomplete": "new-password"
    },
    {
      "key": "rtsp_transport",
      "name": "RTSP Transport Type",
      "type": "select",
      "items": [
        {
          "id": "tcp",
          "title": "TCP"
        },
        {
          "id": "udp",
          "title": "UDP"
        }
      ],
      "defaultValue": "tcp"
    },
    {
      "key": "address2",
      "name": "RTSP Address (Second Channel)",
      "type": "text"
    },
    {
      "key": "external_api",
      "name": "External API",
      "type": "select",
      "driver_types": [43],
      "items": []
    },
    {
      "key": "support_ptz",
      "name": "Support PTZ",
      "type": "checkbox"
    },
    {
      "key": "always_on",
      "name": "Always on",
      "type": "checkbox"
    }
  ]
}